{
    "name": "tiknil/bs-blade-forms",
    "description": "Laravel library of bootstrap blade components for common form creation use cases",
    "keywords": [
        "tiknil",
        "bs-blade-forms"
    ],
    "version": "0.1.0",
    "homepage": "https://github.com/tiknil/bs-blade-forms",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Giorgio Balduzzi",
            "email": "balduzzi.giorgio@tiknil.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/contracts": "^10.0|^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.7",
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Tiknil\\BsBladeForms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tiknil\\BsBladeForms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tiknil\\BsBladeForms\\BsBladeFormsServiceProvider"
            ],
            "aliases": {
                "BsBladeForms": "Tiknil\\BsBladeForms\\BsBladeFormsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
