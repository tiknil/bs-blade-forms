<form method="POST" action="url">
  <input type="hidden" name="_token" value="" autocomplete="off">  <input type="hidden" name="_method" value="PATCH">
  <div >
    <input type="email" id="email" name="email" value="test@example.org" class="form-control" required="required" />
</div>

    <div >
    <input type="password" id="hidden_field" name="hidden_field" value="" class="form-control" required="required" />
</div>


    <div class="form-check">

  <input type="radio"
         value="User"
         name="role"
         id="role_User"
         class="form-check-input" />

  <label for="role_User" class="form-check-label">
      User
  </label>
</div>
    <div class="form-check">

  <input type="radio"
         value="Admin"
         name="role"
         id="role_Admin"
        checked class="form-check-input" />

  <label for="role_Admin" class="form-check-label">
      Admin
  </label>
</div>

    <div class="ss-wrapper"    >

  
  <select name="address[country]" id="address[country]" tabindex="-1" 
      
    class="ss-ghost-select">

    <option  value=""></option>

            <option value="it" selected wire:key="address[country]-it" >Italy</option>
        <option value="de"  wire:key="address[country]-de" >Germany</option>
        <option value="fr"  wire:key="address[country]-fr" >France</option>
        <option value="us"  wire:key="address[country]-us" >United States</option>
     
    
  </select>

  <div wire:ignore="">
    <div role="button" aria-label="" tabindex="0"
      class="form-select ss-box">

      <div class="text-muted ss-placeholder">
                &nbsp;
              </div>
      <div class="ss-value-label" style="display:none;"></div>
    </div>
</div>


  
  <div class="ss-dropdown hidden" >

    <div class="ss-dropdown-search">
      <input type="text" class="form-control form-control-sm "
        placeholder="bs-blade-forms::components.custom-select.search-placeholder" />
    </div>

    
    <template class="ss-option-template">
      <div class="ss-option" data-key="">
        <span></span>
              </div>
    </template>

    
    <div class="ss-options">
    </div>

    <div class="text-muted p-2 empty-results">
      bs-blade-forms::components.custom-select.no-results
    </div>

  </div>
</div>
    <div >
    <input type="text" id="address[city]" name="address[city]" value="Pedrengo" class="form-control" />
</div>

    <div >
    <input type="text" id="address[zipCode]" name="address[zipCode]" value="24066" class="form-control" />
</div>

    <div >
    <input type="text" id="address[province]" name="address[province]" value="BG" class="form-control" />
</div>

    <div >
    <input type="text" id="address[street]" name="address[street]" value="Viale Kennedy" class="form-control" />
</div>

    <div >
    <input type="number" id="address[number]" name="address[number]" value="21" class="form-control" />
</div>


    <div class="ms-wrapper"
           >

  
  <select name="enabled_countries[]"
          tabindex="-1"
          multiple
          id="enabled_countries[]"
                              
          class="ss-ghost-select">

                  <option value="it" selected wire:key="enabled_countries[]-it" >Italy</option>
              <option value="de" selected wire:key="enabled_countries[]-de" >Germany</option>
              <option value="fr"  wire:key="enabled_countries[]-fr" >France</option>
              <option value="us"  wire:key="enabled_countries[]-us" >United States</option>
          
  </select>

  <div wire:ignore="">
    <div role="button"
         aria-label=""
         tabindex="0"
         class="form-select ss-box"
    >

      <div class="text-muted ss-placeholder">
                  &nbsp;
        </div>
      <div class="ss-value-label" style="display:none;"></div>
    </div>
</div>


  
  <div class="ss-dropdown hidden"
         >

    <div class="ss-dropdown-search">

      <input type="text" class="form-control form-control-sm"
             autocomplete="off"
             placeholder="bs-blade-forms::components.custom-select.search-placeholder"/>
      
              <button class="btn btn-light ss-select-all"
                type="button"
                title="bs-blade-forms::components.custom-select.select-all">
          <i class="bi bi-check2-all"></i>
        </button>

        <button class="btn btn-light ss-unselect-all"
                type="button"
                title="bs-blade-forms::components.custom-select.unselect-all">
          <i class="bi bi-dash-square-dotted"></i>
        </button>
          </div>

    
    <template class="ss-option-template">
      <div class="ss-option" data-key="">
        <span></span>

        <i class="bi bi-check-lg ss-check-icon"></i>
      </div>
    </template>

    
    <div class="ss-options">
    </div>

    <div class="text-muted p-2 empty-results">
      bs-blade-forms::components.custom-select.no-results
    </div>

  </div>
</div>
  </form>
